{
  "name": "n17-tooltip",
  "version": "0.0.5",
  "description": "Angular Directive to display a tooltip",
  "repository": {
    "type": "git",
    "url": "https://github.com/wooderz/tooltip-directive.git"
  },
  "keywords": [
    "n17",
    "solutions",
    "angular",
    "directive",
    "tooltip"
  ],
  "author": "James Woodley <info@n17solutions.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wooderz/tooltip-directive/issues"
  },
  "homepage": "https://github.com/wooderz/tooltip-directive",
  "devDependencies": {
    "browserify": "^10.2.1",
    "browserify-shim": "^3.8.7",
    "del": "^1.2.0",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "^1.2.0",
    "run-sequence": "^1.1.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "browser": {
    "angular": "./bower_components/angular/angular.js",
    "jquery": "./node_modules/jquery/dist/jquery.js",
    "qtip2": "./bower_components/qtip2/jquery.qtip.js"
  },
  "browserify": {
    "transform": [
      "browserify-shim"
    ]
  },
  "browserify-shim": {
    "jquery": "jQuery",
    "angular": {
      "exports": "angular",
      "depends": [
        "jquery:jQuery"
      ]
    },
    "qtip2": {
      "depends": [
        "jquery:jQuery"
      ]
    }
  },
  "dependencies": {
    "jquery": "^2.1.4"
  }
}
